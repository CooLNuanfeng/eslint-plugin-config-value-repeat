{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "eslint-plugin-config-value-repeat"
    ],
    "rules": {
        "config-value-repeat/repeat": "error"
    }
}
